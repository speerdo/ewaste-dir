{
  "name": "directory-website",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development astro dev",
    "build": "NODE_ENV=production astro build && node scripts/post-build.js",
    "build:vercel": "chmod +x ./vercel-build.sh && ./vercel-build.sh",
    "build:optimized": "node scripts/optimize-for-build.js && npm run cleanup && NODE_ENV=production astro build && node scripts/post-build.js",
    "postbuild": "node scripts/post-build.js",
    "prebuild": "npm run cleanup",
    "verify": "npm run verify-ssr",
    "verify-ssr": "node scripts/verify-ssr.js",
    "preview": "vercel dev",
    "astro": "astro",
    "import-data": "node scripts/import-data.js",
    "clean": "rm -rf dist node_modules/.astro node_modules/.vite .vercel/output",
    "cleanup": "rm -rf dist/ .vercel/output/ .temp-build/"
  },
  "dependencies": {
    "@astrojs/node": "^9.2.1",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^8.1.4",
    "@astrojs/vue": "^4.0.8",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@supabase/supabase-js": "^2.39.8",
    "@types/google.maps": "^3.55.4",
    "@vercel/analytics": "^1.5.0",
    "astro": "^5.7.2",
    "tailwindcss": "^3.4.1",
    "vue": "^3.4.15"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

{
  "name": "directory-website",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development astro dev",
    "build": "NODE_ENV=production astro build",
    "build:vercel": "NODE_OPTIONS=\"--max-old-space-size=7168\" NODE_ENV=production astro build --verbose",
    "build:optimized": "NODE_OPTIONS=\"--max-old-space-size=7168\" NODE_ENV=production astro build",
    "prebuild": "node scripts/prebuild.js || echo 'Prebuild failed, but continuing with build'",
    "build:full": "npm run prebuild; npm run build:optimized && npm run verify-pages",
    "build:monitor": "node scripts/build-progress.js",
    "verify-pages": "node scripts/verify-pages.js",
    "preview": "astro preview",
    "astro": "astro",
    "import-data": "node scripts/import-data.js",
    "clean": "rm -rf dist node_modules/.astro node_modules/.vite .vercel/output"
  },
  "dependencies": {
    "@astrojs/node": "^9.2.1",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^8.1.4",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@supabase/supabase-js": "^2.39.8",
    "@types/google.maps": "^3.55.4",
    "astro": "^5.7.2",
    "tailwindcss": "^3.4.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

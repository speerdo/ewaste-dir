{
  "name": "directory-website",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development astro dev",
    "build": "NODE_ENV=production node scripts/setup-netlify.js && astro build && node scripts/postbuild-netlify.js",
    "build:netlify": "NODE_ENV=production node scripts/setup-netlify.js && astro build && node scripts/postbuild-netlify.js",
    "build:optimized": "node scripts/optimize-for-build.js && npm run prepare && NODE_OPTIONS=\"--max-old-space-size=7168 --expose-gc\" NODE_ENV=production astro build",
    "prebuild": "node scripts/setup-netlify.js && npm run prepare",
    "prepare": "rm -rf .netlify/functions/ .temp-build/ node_modules/.astro node_modules/.vite",
    "verify": "npm run verify-ssr",
    "verify-ssr": "node scripts/verify-ssr.js",
    "check-functions": "node scripts/check-netlify-functions.js",
    "preview": "netlify dev",
    "astro": "astro",
    "import-data": "node scripts/import-data.js",
    "clean": "rm -rf dist node_modules/.astro node_modules/.vite .netlify/functions",
    "cleanup": "rm -rf .netlify/functions/ .temp-build/"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.3.2",
    "@astrojs/tailwind": "^5.1.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@supabase/supabase-js": "^2.39.8",
    "@types/google.maps": "^3.55.4",
    "astro": "^5.7.2",
    "glob": "^10.4.5",
    "tailwindcss": "^3.4.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

{
  "github": {
    "silent": true
  },
  "buildCommand": "npm run build:vercel",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "astro",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/api/zipfind",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-store"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-store"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Accept, X-Requested-With, X-No-Cache"
        },
        {
          "key": "Surrogate-Control",
          "value": "no-store"
        },
        {
          "key": "Edge-Control",
          "value": "no-store"
        },
        {
          "key": "Vary",
          "value": "*"
        },
        {
          "key": "X-Vercel-Skip-Cache",
          "value": "true"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0, s-maxage=0, proxy-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Accept, X-Requested-With, X-No-Cache, X-Zip-Code, X-Request-ID"
        },
        {
          "key": "Surrogate-Control",
          "value": "no-store"
        },
        {
          "key": "Edge-Control",
          "value": "no-store"
        },
        {
          "key": "Vary",
          "value": "*"
        },
        {
          "key": "X-Vercel-Skip-Cache",
          "value": "true"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/zipfind",
      "destination": "/api/zipfind"
    },
    {
      "source": "/api/zipcode",
      "destination": "/api/zipcode"
    },
    {
      "source": "/api/cities-data",
      "destination": "/api/cities-data"
    }
  ],
  "regions": ["iad1"],
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=7168 --no-warnings"
    }
  }
}
